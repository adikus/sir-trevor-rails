<div class="content-block instagram-block">
  <%=
    if block['instagram_embed_code'] && block['instagram_embed_code'].html_safe.match(/\<blockquote (.*)\<\/blockquote>(\s+)\<script async defer src=\"www\.instagram\.com\/embed\.js\">\<\/script\>/)
      block['instagram_embed_code'].html_safe
    else
      ''
    end
  %>
</div>
